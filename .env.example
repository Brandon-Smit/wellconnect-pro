# Application Configuration
NODE_ENV=production
PORT=3000

# WellConnect Pro - Environment Configuration Example

# API Keys (REPLACE WITH YOUR ACTUAL KEYS)
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
HUGGING_FACE_API_KEY=your_hugging_face_api_key_here
LINKEDIN_API_KEY=your_linkedin_api_key_here
CRUNCHBASE_API_KEY=your_crunchbase_api_key_here
DISCOVERY_API_KEY=your_email_discovery_api_key_here

# Email Service Credentials
GMAIL_USERNAME=your_gmail_username@gmail.com
GMAIL_PASSWORD=your_gmail_app_password
SENDGRID_USERNAME=your_sendgrid_username
SENDGRID_PASSWORD=your_sendgrid_password

# SMTP Configurations (Backup Email Services)
SMTP_USER_1=smtp_primary_username
SMTP_PASS_1=smtp_primary_password
SMTP_USER_2=smtp_secondary_username
SMTP_PASS_2=smtp_secondary_password

# Database Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/wellconnect?retryWrites=true&w=majority

# API and Frontend Configuration
NEXT_PUBLIC_API_BASE_URL=https://wellconnect-pro.vercel.app/api
NEXT_PUBLIC_API_URL=https://wellconnect-pro.vercel.app/api
FRONTEND_URL=https://wellconnect-pro.vercel.app

# Authentication
JWT_SECRET=generate_a_long_random_secret_key_at_least_32_characters

# Monitoring and Logging
SENTRY_DSN=your_sentry_project_dsn
LOG_LEVEL=info
PERFORMANCE_TRACKING_ENABLED=true

# Proxy Configuration (Optional)
PROXY_SERVERS=proxy1.example.com,proxy2.example.com

# Environment Settings
NEXT_PUBLIC_APP_ENV=production
VERCEL_ENV=production

# Ethical AI and Compliance
NEXT_PUBLIC_ETHICAL_MARKETING_MODE=true
NEXT_PUBLIC_COMPLIANCE_LEVEL=high

# Additional Security Settings
CORS_ALLOWED_ORIGINS=https://wellconnect-pro.vercel.app,http://localhost:3000

# Feature Flags
ENABLE_AI_CONTENT_GENERATION=true
ENABLE_EMAIL_TRACKING=true
ENABLE_COMPLIANCE_CHECKS=true

# Affiliate Context Analysis
AFFILIATE_CONTEXT_API_KEY=your_affiliate_context_api_key
AFFILIATE_CONTEXT_ENDPOINT=https://api.contextanalyzer.com/v1

# Machine Learning Model Configuration
ML_MODEL_ENDPOINT=https://ml.wellconnectpro.com/predict
ML_MODEL_API_KEY=your_ml_model_api_key

# Logging and Monitoring
ERROR_TRACKING_DSN=https://your-error-tracking-dsn

# Performance Tracking
PERFORMANCE_TRACKING_ENDPOINT=https://tracking.wellconnectpro.com/v1

# Ethical AI Guidelines
ETHICAL_SCORE_THRESHOLD=0.75
MAX_PERSONALIZATION_DEPTH=3

# Email Service Configuration
EMAIL_SERVICE_PROVIDER=sendgrid
SENDGRID_API_KEY=your_sendgrid_api_key

# Security Configuration
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Email Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USERNAME=your_username
SMTP_PASSWORD=your_password
EMAIL_DISPATCH_RATE_LIMIT=50
EMAIL_DAILY_QUOTA=500

# Performance Monitoring
CPU_USAGE_THRESHOLD=80
MEMORY_USAGE_THRESHOLD=85

# Compliance Settings
GDPR_COMPLIANCE_ENABLED=true
CAN_SPAM_COMPLIANCE_ENABLED=true
CONSENT_MANAGEMENT_ENABLED=true

# Affiliate Configuration
AFFILIATE_TRACKING_ENABLED=true
AFFILIATE_COMMISSION_RATE=0.10
AFFILIATE_ETHICAL_SCORE_THRESHOLD=0.7

# Logging
LOG_DIRECTORY=./logs

# Security
ENCRYPTION_KEY=your_secure_encryption_key
